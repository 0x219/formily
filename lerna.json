{
  "packages": [
    "packages/!(devtools)"
  ],
  "version": "independent",
  "command": {
    "bootstrap": {
      "npmClientArgs": [
        "--no-package-lock"
      ]
    },
    "version": {
      "allowBranch": ["master","uform_v1"]
    },
    "publish": {
      "allowBranch": "master",
      "ignoreChanges": [
        "*.md"
      ]
    }
  }
}